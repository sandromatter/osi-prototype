<!-- declaring var in Nunjucks: use `set` keyword -->
{% set navsCategory = [
    { title: 'Gastro', url: 'category.html', state: 'gastro' },
    { title: 'Über', url: 'category.html', state: 'ueber' },
    { title: 'Kontakt', url: 'category.html', state: 'kontakt' },
    { title: 'Gastro', url: 'category.html', state: 'gastro' },
    { title: 'Über', url: 'category.html', state: 'ueber' },
    { title: 'Kontakt', url: 'category.html', state: 'kontakt' },
    { title: 'Gastro', url: 'category.html', state: 'gastro' },
    { title: 'Über', url: 'category.html', state: 'ueber' },
    { title: 'Kontakt', url: 'category.html', state: 'kontakt' }
] %}

<nav class="navbar navbar-category">
    <ul class="navbar-category-list mb-0 pl-0">
        <!-- for loop in Nunjucks -->
        {% for nav in navsCategory %}
            {% if page == nav.state %}
            <li class="active">
                <a class="nav-item nav-link" href="{{ nav.url }}">{{ nav.title }}<span class="sr-only">(current)</span></a>
            {% else %}
            <li>
                <a class="nav-item nav-link" href="{{ nav.url }}">{{ nav.title }}</a>
            {% endif %}
            </li>
        {% endfor %}
    </ul>
</nav>